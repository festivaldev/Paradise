<?xml version="1.0" encoding="Windows-1252"?>

<Configuration>
	<Paradise>
		<UDPListeners>
			<UDPListener
				IPAddress="0.0.0.0"
				Port="5055"
				OverrideApplication="Paradise.Realtime.Server.Comm">
			</UDPListener>
			
			<UDPListener
				IPAddress="0.0.0.0"
				Port="5056"
				OverrideApplication="Paradise.Realtime.Server.Game">
			</UDPListener>
		</UDPListeners>

		<Runtime
			Assembly="PhotonHostRuntime, Culture=neutral"
			Type="PhotonHostRuntime.PhotonDomainManager"
			UnhandledExceptionPolicy="Ignore">
		</Runtime>

		<Applications Default="Paradise.Realtime.Server.Game">
			<Application
				Name="Paradise.Realtime.Server.Comm"
				BaseDirectory="Paradise.Realtime.Server.Comm"
				Assembly="Paradise.Realtime.Server.Comm"
				Type="Paradise.Realtime.Server.Comm.CommApplication"
				ForceAutoRestart="true"
				WatchFiles="dll;config"
				ExcludeFiles="log4net.config">
			</Application>
			
			<Application
				Name="Paradise.Realtime.Server.Game"
				BaseDirectory="Paradise.Realtime.Server.Game"
				Assembly="Paradise.Realtime.Server.Game"
				Type="Paradise.Realtime.Server.Game.GameApplication"
				ForceAutoRestart="true"
				WatchFiles="dll;config"
				ExcludeFiles="log4net.config">
			</Application>
		</Applications>
	</Paradise>
</Configuration>
