<?xml version="1.0" encoding="utf-8" ?>
<ParadiseDiscordSettings>
	<EnableDiscordIntegration>true</EnableDiscordIntegration>
	<Token><!-- Insert Discord Application Token here --></Token>

	<ChatIntegration>true</ChatIntegration>
	<CommandIntegration>true</CommandIntegration>
	<PlayerJoinAnnouncements>true</PlayerJoinAnnouncements>
	<PlayerLeaveAnnouncements>true</PlayerLeaveAnnouncements>
	<RoomOpenAnnouncements>true</RoomOpenAnnouncements>
	<RoomCloseAnnouncements>true</RoomCloseAnnouncements>
	<ErrorLog>true</ErrorLog>

	<GuildId><!-- Insert Server (Guild) ID here --></GuildId>
	<ChatChannelId><!-- Insert Chat channel ID here --></ChatChannelId>
	<CommandChannelId><!-- Insert Command channel ID here --></CommandChannelId>

	<ChatWebHookUrl></ChatWebHookUrl>
	<PlayerAnnouncementWebHookUrl></PlayerAnnouncementWebHookUrl>
	<GameAnnouncementWebHookUrl></GameAnnouncementWebHookUrl>
	<ErrorLogWebHookUrl></ErrorLogWebHookUrl>
</ParadiseDiscordSettings>